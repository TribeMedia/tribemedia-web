# PostgreSQL
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=postgres

# Kong
KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443

# API
API_EXTERNAL_URL=http://localhost:8000

# Studio
STUDIO_DEFAULT_ORGANIZATION=Tribe
STUDIO_DEFAULT_PROJECT=Prometheus

# Auth
SITE_URL=http://localhost:3000
ADDITIONAL_REDIRECT_URLS=
DISABLE_SIGNUP=false
JWT_EXPIRY=3600
JWT_SECRET=your_jwt_secret
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_SIGNUP=true
ENABLE_PHONE_AUTOCONFIRM=false
ENABLE_ANONYMOUS_USERS=true

# SMTP
SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=smtp_user
SMTP_PASS=smtp_password
SMTP_SENDER_NAME=Your Sender Name

# Auth Redirects
MAILER_URLPATHS_INVITE=http://localhost:3000/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=http://localhost:3000/auth/v1/verify
MAILER_URLPATHS_RECOVERY=http://localhost:3000/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=http://localhost:3000/auth/v1/verify

# REST
PGRST_DB_SCHEMAS=public,storage,graphql_public

# Storage
ANON_KEY=your_anon_key
SERVICE_ROLE_KEY=your_service_role_key

# ImageProxy
IMGPROXY_ENABLE_WEBP_DETECTION=true

# Functions
FUNCTIONS_VERIFY_JWT=false

# Analytics
LOGFLARE_API_KEY=your_logflare_api_key

# Supabase
SUPABASE_PUBLIC_URL=http://localhost:8000

# Dashboard
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=admin

# Docker
DOCKER_SOCKET_LOCATION=/Users/gqadonis/.docker/run/docker.sock

# Zitadel (if needed)
ZITADEL_MASTERKEY=MasterkeyNeedsToHave32Characters